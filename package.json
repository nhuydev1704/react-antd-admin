  "devDependencies": {
    "@iconify/json": "2.2.245",
    "@iconify/types": "2.0.0",
    "@ohh-889/react-auto-route": "0.3.5",
    "@sa/scripts": "workspace:*",
    "@sa/uno-preset": "workspace:*",
    "@soybeanjs/eslint-config": "1.4.1",
    "@svgr/core": "8.1.0",
    "@svgr/plugin-jsx": "8.1.0",
    "@types/gradient-string": "1.1.6",
    "@types/node": "22.5.4",
    "@types/nprogress": "0.2.3",
